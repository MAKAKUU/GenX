{
	"manifest_version": 3,
	"name": "GenX - AI Studio",
	"version": "1.0.0",
	"description": "A browser-based AI UI & Studio using Svelte.",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"permissions": [],
	"action": {
		"default_title": "GenX"
	},
	"host_permissions": [],
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self';"
	}
}
